{
	// Enforcing
	"bitwise": true,
	"camelcase": false,
	"eqeqeq": true,
	"forin": false,
	"immed": false,
	"latedef": false,
	"newcap": false,
	"noarg": true,
	"noempty": true,
	"nonew": true,
	"plusplus": false,
	"quotmark": false,
	"undef": true,
	"unused": true,
	"strict": false,
	"trailing": true,

	"maxparams": 4,
	"maxdepth": 3,
	"maxcomplexity": 5,

	// Relaxing
	"asi": false,
	"boss": false,
	"debug": false,
	"eqnull": true,
	"esnext": false,
	"evil": false,
	"expr": false,
	"funcscope": true,
	"globalstrict": false,
	"iterator": false,
	"lastsemic": false,
	"laxbreak": true,
	"laxcomma": true,
	"loopfunc": true,
	"multistr": false,
	"proto": false,
	"scripturl": false,
	"smarttabs": true,
	"shadow": false,
	"sub": false,
	"supernew": false,
	"validthis": true,

	// Environments
	"browser": true,
	"devel": true,
	"node": true,
	"worker": true,

	// Globals
	"globals": {
		"NodeList": false
	}
}
